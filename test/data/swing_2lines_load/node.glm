// 4 Node Feeder: swing bus, with line to node, with line to load

/////////////////////////////////////////////
// BEGIN
/////////////////////////////////////////////

module tape;

clock {
    timestamp '2000-01-01 0:00:00';
    timezone EST+5EDT;
}

module powerflow;

// object voltdump {
//     filename swing_2lines_load_voltage.csv;
// 	mode RECT;
// }

object overhead_line_conductor:100 {
    geometric_mean_radius 0.0244;
    resistance 0.306;
    // diameter 0.721;
}

object overhead_line_conductor:101 {
    geometric_mean_radius 0.00814;
    resistance 0.592;
    // diameter 0.563;
}

object line_spacing:200 {
    distance_AB 2.5;
    distance_BC 4.5;
    distance_AC 7.0;
    distance_AN 5.656854;
    distance_BN 4.272002;
    distance_CN 5.0;
}

object line_configuration:300 {
    conductor_A overhead_line_conductor:100;
    conductor_B overhead_line_conductor:100;
    conductor_C overhead_line_conductor:100;
    conductor_N overhead_line_conductor:101;
    spacing line_spacing:200;
}

object node {
    name node1;
    phases "ABCN";
    voltage_A +7199.558+0.000j;
    voltage_B -3599.779-6235.000j;
    voltage_C -3599.779+6235.000j;
    nominal_voltage 7200;
    bustype SWING;
    // object recorder {
    //     file node1_voltages.csv;
    //     interval 60;
    //     property voltage_A, voltage_B, voltage_C;
    // };
}

object overhead_line:12 {
    phases "ABCN";
    from node1;
    to node2;
    length 2000;
    configuration line_configuration:300;
}

object node {
    name node2;
    phases "ABCN";
    voltage_A +7199.558+0.000j;
    voltage_B -3599.779-6235.000j;
    voltage_C -3599.779+6235.000j;
    nominal_voltage 7200;
    // object recorder {
    //     file node2_voltages.csv;
    //     interval 60;
    //     property voltage_A, voltage_B, voltage_C;
    // };
}

object overhead_line:23 {
    phases "ABCN";
    from node2;
    to node3;
    length 2000;
    configuration line_configuration:300;
}

object node {
    name node3;
    phases "ABCN";
    voltage_A +7199.558+0.000j;
    voltage_B -3599.779-6235.000j;
    voltage_C -3599.779+6235.000j;
    nominal_voltage 7200;
    // object recorder {
    //     file node3_voltages.csv;
    //     interval 60;
    //     property voltage_A, voltage_B, voltage_C;
    // };
}

object overhead_line:34 {
    phases "ABCN";
    from node3;
    to load4;
    length 2500;
    configuration line_configuration:300;
}

object load {
    name load4;
    phases "ABCN";
    constant_power_A +1800000.000+871779.789j;
    constant_power_B +1800000.000+871779.789j;
    constant_power_C +1800000.000+871779.789j;
    nominal_voltage 7200;
    // object recorder {
    //     file load4_voltages.csv;
    //     interval 60;
    //     property measured_voltage_A, measured_voltage_B, measured_voltage_C;
    // };
}


///////////////////////////////
// END
///////////////////////////////


object voltdump {
     filename result.csv;
     mode RECT;
}
