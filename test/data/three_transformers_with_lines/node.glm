// An infinite source, transformers, and a PQ load

/////////////////////////////////////////////
// BEGIN
/////////////////////////////////////////////

module tape;

module powerflow {
    solver_method NR;
};

// object voltdump {
//     filename voltage.csv;
// 	mode RECT;
// }

object transformer_configuration:400 {
    connect_type 1;
    power_rating 6000;
    powerA_rating 2000;
    powerB_rating 2000;
    powerC_rating 2000;
    primary_voltage 12470;
    secondary_voltage 4160;
    resistance 0.01;
    reactance 0.06;
}

object transformer_configuration:450 {
    connect_type 1;
    power_rating 6000;
    powerA_rating 2000;
    powerB_rating 2000;
    powerC_rating 2000;
    primary_voltage 4160;
    secondary_voltage 12470;
    resistance 0.01;
    reactance 0.06;
}
// Phase Conductor for 601: 556,500 26/7 ACSR
object overhead_line_conductor {
	name olc6010;
	geometric_mean_radius 0.031300;
	diameter 0.927 in;
	resistance 0.185900;
}

// Phase Conductor for 602: 4/0 6/1 ACSR
object overhead_line_conductor {
	name olc6020;
	geometric_mean_radius 0.00814;
	diameter 0.56 in;
	resistance 0.592000;
}

object line_spacing {
	name ls500601;
	distance_AB 2.5;
	distance_AC 4.5;
	distance_BC 7.0;
	distance_BN 5.656854;
	distance_AN 4.272002;
	distance_CN 5.0;
	distance_AE 28.0;
	distance_BE 28.0;
	distance_CE 28.0;
	distance_NE 24.0;
}

object line_configuration {
	name lc601;
	conductor_A olc6010;
	conductor_B olc6010;
	conductor_C olc6010;
	conductor_N olc6020;
	spacing ls500601;
}

object node {
    name node1;
    phases "ABCN";
    nominal_voltage 7200;
    bustype SWING;
}

object transformer:12 {
    phases "ABCN";
    from node1;
    to node2;
    configuration transformer_configuration:400;
}


object node {
    name node2;
    phases "ABCN";
    nominal_voltage 2400;
}

object overhead_line { 
     phases "ABCN";
     name line_2-3;
     from node2;
     to node3;
     length 2000;
     configuration lc601;
}

object node {
    name node3;
    phases "ABCN";
    nominal_voltage 2400;
}

object transformer:34 {
    phases "ABCN";
    from node3;
    to node4;
    configuration transformer_configuration:450;
}

object node {
    name node4;
    phases "ABCN";
    nominal_voltage 7200;
}

object overhead_line { 
     phases "ABCN";
     name line_4-5;
     from node4;
     to node5;
     length 2000;
     configuration lc601;
}

object node {
    name node5;
    phases "ABCN";
    nominal_voltage 7200;
}

object transformer:51 {
    phases "ABCN";
    from node5;
    to load1;
    configuration transformer_configuration:400;
}


object load {
    name load1;
    phases "ABCN";
    constant_power_A +1800000.000+871779.789j;
    constant_power_B +1800000.000+871779.789j;
    constant_power_C +1800000.000+871779.789j;
    nominal_voltage 2400;
    bustype PQ;
}


object voltdump {
     filename result.csv;
     mode RECT;
}



clock {
	timezone EST+8EDT;
	timestamp '2000-01-01 0:00:00';
	stoptime '2000-01-01 0:00:01';
}
