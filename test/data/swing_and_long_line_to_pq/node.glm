// An infinite source, a line, and a PQ load

/////////////////////////////////////////////
// BEGIN
/////////////////////////////////////////////

module tape;

clock {
    timestamp '2000-01-01 0:00:00';
    timezone EST+5EDT;
}

module powerflow {
    solver_method NR;
    NR_matrix_file "y_matrix_printout.txt";
    NR_matrix_output_interval PER_CALL;
    NR_matrix_output_references true;
};

// object voltdump {
//     filename swing_and_long_line_to_pq_voltage.csv;
// 	mode RECT;
// }

object overhead_line_conductor:100 {
    geometric_mean_radius 0.0244;
    resistance 0.306;
    diameter 0.721;
}

object overhead_line_conductor:101 {
    geometric_mean_radius 0.00814;
    resistance 0.592;
    diameter 0.563;
}

object line_spacing:200 {
    distance_AB 2.5;
    distance_BC 4.5;
    distance_AC 7.0;
    distance_AN 5.656854;
    distance_BN 4.272002;
    distance_CN 5.0;
}

object line_configuration:300 {
    conductor_A overhead_line_conductor:100;
    conductor_B overhead_line_conductor:100;
    conductor_C overhead_line_conductor:100;
    conductor_N overhead_line_conductor:101;
    spacing line_spacing:200;
}

object node {
    name node1;
    phases "ABCN";
    nominal_voltage 2400;
    bustype SWING;
    // object recorder {
    //     file node1_voltages.csv;
    //     interval 60;
    //     property voltage_A, voltage_B, voltage_C;
    // };
}

object overhead_line:12 {
    phases "ABCN";
    from node1;
    to load1;
    length 6000;
    configuration line_configuration:300;
}

object load {
    name load1;
    phases "ABCN";
    constant_power_A +1800000.000+871779.789j;
    constant_power_B +1800000.000+871779.789j;
    constant_power_C +1800000.000+871779.789j;
    nominal_voltage 2400;
    bustype PQ;
    // object recorder {
    //     file load1_voltages.csv;
    //     interval 10000;
    //     property measured_voltage_A, measured_voltage_B, measured_voltage_C;
    // };
}


object voltdump {
     filename result.csv;
     mode RECT;
}
